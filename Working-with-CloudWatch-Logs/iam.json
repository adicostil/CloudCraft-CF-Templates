{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "cloudtrail:*",
                "cloudwatch:*",
                "ec2:*",
                "es:*",
                "iam:Get*",
                "iam:List*",
                "logs:*",
                "route53:*",
                "s3:*"
            ],
            "Resource": "*"
        }
    ]
}