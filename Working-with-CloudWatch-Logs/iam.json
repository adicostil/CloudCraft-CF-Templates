{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "cloudtrail:*",
                "cloudwatch:*",
                "ec2:*",
                "es:*",
                "iam:Get*",
                "iam:List*",
                "iam:PassRole",
                "iam:PutRolePolicy",
                "kms:ListKeys",
                "kms:ListAliases",
                "logs:*",
                "route53:*",
                "s3:*",
                "sns:*"
            ],
            "Resource": "*"
        }
    ]
}