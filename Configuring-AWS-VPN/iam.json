{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "CloudwatchFullAccess",
            "Effect": "Allow",
            "Action": [
                "autoscaling:*",
                "cloudwatch:*",
                "ec2:*",
                "elasticloadbalancing:*",
                "events:*",
                "sns:*"
            ],
            "Resource": "*"
        }
    ]
}